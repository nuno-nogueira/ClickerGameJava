<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="153.0" prefWidth="256.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/25" fx:controller="com.clickergame.ui.MainController">
   <StackPane prefHeight="50.0" prefWidth="400.0" StackPane.alignment="BOTTOM_CENTER">
      <children>
         <Button fx:id="upgradeButton" alignment="BOTTOM_CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#onUpgradeButton" text="Upgrade Button" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
               <Insets bottom="5.0" />
            </StackPane.margin>
         </Button>
         <Button fx:id="cursorButton" mnemonicParsing="false" onAction="#onCursorUpgradeButton" text="Cursor Upgrade Button" StackPane.alignment="BOTTOM_LEFT">
            <StackPane.margin>
               <Insets bottom="5.0" />
            </StackPane.margin>
         </Button>
      </children>
   </StackPane>
   <Label fx:id="coinsLabel" alignment="TOP_CENTER" text="Coins" StackPane.alignment="TOP_CENTER" />
    <Button fx:id="clickButton" alignment="CENTER" onAction="#onClickButton" text="Cookie Button" />
   <Label fx:id="cursorLabel" text="Cursors" StackPane.alignment="TOP_CENTER">
      <StackPane.margin>
         <Insets top="20.0" />
      </StackPane.margin>
   </Label>
</StackPane>
